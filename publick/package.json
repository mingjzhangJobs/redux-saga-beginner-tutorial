  "scripts": {
    "dll": "webpack -p --config webpack/webpack.dll.config.js --progress --profile --colors",
    "start": "node webpack/server.js",
    "server": "cross-env NODE_ENV=production node webpack/server.js",
    "prettier": "prettier --write \"{src,mock}/**/*.{js,jsx,css,less}\"",
    "build:analysis": "cross-env NODE_ENV=production webpack -p --config webpack/webpack.prd.config.js --profile --analysis --colors --display errors-only",
