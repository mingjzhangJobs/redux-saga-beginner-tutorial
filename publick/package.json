    "build": "cross-env NODE_ENV=production webpack -p --config webpack/webpack.prd.config.js --profile --colors --display errors-only",
    "gulp": "cross-env NODE_ENV=production  gulp build",
    "gulp:prod": "cross-env NODE_ENV=production  gulp build --terserconfig"
  },
